{
  "schemaVersion": 1,
  "id": "reforested",
  "version": "${version}",

  "name": "Reforested",
  "description": "A mod inspired by Forestry Mod.",
  "authors": [
    "Prefex",
    "Falkory"
  ],
  "contact": {
    "homepage": "https://prefex.dev/",
    "sources": "https://github.com/DcZipPL/Reforested"
  },

  "license": "GPL-3",
  "icon": "assets/reforested/icon.png",

  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "dev.prefex.reforested.datagen.ReforestedDataGenerator"
    ],
    "main": [
      "dev.prefex.reforested.Reforested"
    ],
    "client": [
      "dev.prefex.reforested.ReforestedClient"
    ]
  },
  "mixins": [
    "reforested.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.22",
    "fabric-api": "*",
    "minecraft": "~1.20.1",
    "java": ">=17"
  }
}
